#!groovy DSL
// Hello! Michael!!!

pipeline {
  
	agent any
	
	options{
		timestamps()
	}

	stages {
		stage('My-JenkinsBuild-Steps') {
			steps {
				echo "Start"
				echo "Doing something.."
				echo "End"
				
				echo powershell(returnStdout: true, script: """ 
				Write-Output "PowerShell is mighty!"
				Write-Output " "
				dir
				Write-Output " "
				Write-Output " "
				Write-Output " "
				
				$myvar = "mika-pika-privet!!!"
				
				Write-Output  "$myvar"
				""")
				
			}
		}
	}
}